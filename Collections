fun main(){
    val myList  = listOf("James", "Paul", "Raf", "Gina")
    
    myList.forEach {
        println(it)
    }
    
    print(myList)
    print("\n")
    for (i in myList){
        print(i+" ")
    }
    print("\n")
    for (i in myList){
        println(i)
    }
    print("\n")
    for (i in myList){
        println("Name: $i")
    }
    
}
